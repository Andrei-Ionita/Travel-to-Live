    <script type="text/javascript">

		$('#google-map1').gMap({

			 address: 'Melbourne, Australia',
			 maptype: 'ROADMAP',
			 zoom: 14,
			 markers: [
				{
					address: "Melbourne, Australia"
				}
			 ],
			 doubleclickzoom: false,
			 controls: {
